# This is Git's per-user configuration file.
[alias]
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[credential]
	helper = store
[pull]
	rebase = false
[init]
	defaultBranch = master
[core]
	pager = less -F -X
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	gpgsign = true
